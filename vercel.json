{
    "version": 2,
    "buildCommand": "npm install && npx expo export",
    "outputDirectory": "dist",
    "routes": [
      { 
        "src": "/(.*)", 
        "dest": "/$1" 
      },
      { 
        "handle": "filesystem"
      },
      { 
        "src": "/.*", 
        "dest": "/index.html" 
      }
    ]
  }