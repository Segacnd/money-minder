{
    "version": 2,
    "buildCommand": "cd money-minder && npm install && npx expo export",
    "outputDirectory": "money-minder/dist",
    "routes": [
      { 
        "src": "/(.*)", 
        "dest": "/$1" 
      },
      { 
        "handle": "filesystem"
      },
      { 
        "src": "/.*", 
        "dest": "/index.html" 
      }
    ]
  }